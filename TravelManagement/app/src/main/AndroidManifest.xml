<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="seu.ac.bd.travelmanagement">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TravelDetails" />
        <activity android:name=".AddTrip" />
        <activity android:name=".VisitToChittagong.Chittagong" />
        <activity android:name=".PlaceOfChittagong.WhatToDoChittagong" />
        <activity android:name=".VisitToChittagong.PlaceOfChittagong" />
        <activity android:name=".VisitToChittagong.FoodOfChittagong" />
        <activity android:name=".VisitToSylhet.Sylhet" />
        <activity android:name=".VisitToSylhet.FoodOfSylhet" />
        <activity android:name=".VisitToSylhet.PlaceOfSylhet" />
        <activity android:name=".VisitToSylhet.SavedPlaceOfSylhet" />
    </application>

</manifest>